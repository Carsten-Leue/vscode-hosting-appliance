{
  "name": "hosting-appliance",
  "version": "0.0.4",
  "description": "Code development for hosting appliance",
  "repository": {
    "type": "git",
    "url": "git@github.com:Carsten-Leue/vscode-hosting-appliance.git"
  },
  "license": "MIT",
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "deploy": "vsce publish --yarn",
    "lint": "eslint src --ext ts",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "yarn run compile",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "cross-spawn": "^7.0.2",
    "fs-extra": "^9.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.1",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vsce": "^1.75.0",
    "vscode-test": "^1.3.0"
  },
  "engines": {
    "vscode": "^1.44.0"
  },
  "activationEvents": [
    "onCommand:hosting-appliance.helloWorld"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "hosting-appliance.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "displayName": "hosting-appliance",
  "icon": "images/icon.png",
  "preview": true,
  "publisher": "drcarstenleue"
}
